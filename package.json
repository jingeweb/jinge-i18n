{
  "name": "jinge-i18n",
  "version": "1.0.0",
  "description": "i18n solution for jinge mvvm framework",
  "main": "lib/index.js",
  "types": "src/index.ts",
  "bin": {
    "jinge-i18n": "./bin/jinge-i18n.js"
  },
  "scripts": {
    "build": "rm -rf ./lib && rm -rf ./compiler && tsc && rm -rf .tmp && node ./scripts/esbuild.js",
    "dev": "WATCH=true npm run build",
    "lint": "tsc --noEmit && eslint './{src,scripts}/**/*.{js,ts}' --fix",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jingeweb/jinge-i18n.git"
  },
  "license": "ISC",
  "homepage": "https://github.com/jingeweb/jinge-i18n#readme",
  "bugs": {
    "url": "https://github.com/jingeweb/jinge-i18n/issues"
  },
  "keywords": [
    "mvvm",
    "spa"
  ],
  "author": "Yuhang Ge <abeyuhang@gmail.com>",
  "dependencies": {
    "@jingeweb/html5parser": "^0.0.4",
    "acorn": "^8.8.0",
    "acorn-walk": "^8.2.0",
    "csv-parse": "^5.3.0",
    "csv-stringify": "^6.2.0",
    "diff": "^5.1.0",
    "glob": "^8.0.3",
    "yargs": "^17.5.1"
  },
  "peerDependencies": {
    "jinge": ">= 3.2.2"
  },
  "devDependencies": {
    "@types/estree": "^0.0.52",
    "@types/glob": "^7.2.0",
    "@types/node": "^18.7.4",
    "@types/svg-tags": "^1.0.0",
    "@types/yargs": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "chokidar": "^3.5.3",
    "esbuild": "^0.15.3",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.4.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jinge": "^3.2.2",
    "jinge-compiler": "^3.2.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0"
  }
}
